#: src/global.c:582
msgid "Go forward one character"
msgstr "Avansează cu un caracter"

#: src/global.c:583
msgid "Go back one word"
msgstr "Înapoi cu un cuvânt"

#: src/global.c:584
msgid "Go forward one word"
msgstr "Avansează cu un cuvânt"

